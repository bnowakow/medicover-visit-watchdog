<%- model_class = MedicoverApi -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:url) %>:</strong></dt>
  <dd><%= @medicover_api.url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:token) %>:</strong></dt>
  <dd><%= @medicover_api.token %></dd>
  <dt><strong><%= model_class.human_attribute_name(:new_visit_path) %>:</strong></dt>
  <dd><%= @medicover_api.new_visit_path %></dd>
  <dt><strong><%= model_class.human_attribute_name(:refresh_token_path) %>:</strong></dt>
  <dd><%= @medicover_api.refresh_token_path %></dd>
  <dt><strong><%= model_class.human_attribute_name(:username) %>:</strong></dt>
  <dd><%= @medicover_api.username %></dd>
  <dt><strong><%= model_class.human_attribute_name(:password) %>:</strong></dt>
  <dd><%= @medicover_api.password %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              medicover_apis_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_medicover_api_path(@medicover_api), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              medicover_api_path(@medicover_api),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
