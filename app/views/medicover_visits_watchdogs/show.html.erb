<%- model_class = MedicoverVisitsWatchdog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:medicover_visit_query_id) %>:</strong></dt>
  <dd><%= @medicover_visits_watchdog.medicover_visit_query_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:first_possible_appointment_date) %>:</strong></dt>
  <dd><%= @medicover_visits_watchdog.first_possible_appointment_date %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              medicover_visits_watchdogs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_medicover_visits_watchdog_path(@medicover_visits_watchdog), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              medicover_visits_watchdog_path(@medicover_visits_watchdog),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
