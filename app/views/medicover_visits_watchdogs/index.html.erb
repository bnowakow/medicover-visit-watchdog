<%- model_class = MedicoverVisitsWatchdog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:medicover_visit_query_id) %></th>
      <th><%= model_class.human_attribute_name(:first_possible_appointment_date) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @medicover_visits_watchdogs.each do |medicover_visits_watchdog| %>
      <tr>
        <td><%= link_to medicover_visits_watchdog.id, medicover_visits_watchdog_path(medicover_visits_watchdog) %></td>
        <td><%= medicover_visits_watchdog.medicover_visit_query_id %></td>
        <td><%= medicover_visits_watchdog.first_possible_appointment_date %></td>
        <td><%=l medicover_visits_watchdog.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_medicover_visits_watchdog_path(medicover_visits_watchdog), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      medicover_visits_watchdog_path(medicover_visits_watchdog),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_medicover_visits_watchdog_path,
            :class => 'btn btn-primary' %>
